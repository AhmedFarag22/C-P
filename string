#include<iostream>
#include<string>
using namespace std;


int main() {

	string x = "ahmed", y="ahmed12345";
	y.assign(x);//x in y
	getline(cin, x);
	cout << x;
	x.at(0);//a
	x.substr(1, 3);//hme
	x.substr(1);//med
	x.find("a");
	x.erase(0, 2);//ed
	x.erase(2);//ed
	y.replace(5, 4, "ali");//move 5 steps and replace the next 4 steps with ali
	y.insert(5, " gamal");//move 5 steps and insert gamal

}
